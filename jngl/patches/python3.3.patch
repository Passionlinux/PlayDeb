@@ -103,9 +103,9 @@
 	if env['python']:
 		env = env.Clone()
 		env.ParseConfig("pkg-config --cflags --libs jngl.pc")
-		env.Append(CPPPATH="/usr/include/python3.2",
+		env.Append(CPPPATH="/usr/include/python3.5m",

--- freeminer-0.4.14.8.orig/src/CMakeLists.txt	2016-05-16 01:01:26.624546499 +0200
+++ freeminer-0.4.14.8/src/CMakeLists.txt	2016-05-16 01:01:26.620546427 +0200
@@ -566,10 +566,6 @@
 	# Unix probably
 	if(BUILD_CLIENT)
 		find_package(X11 REQUIRED)
